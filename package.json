{
  "name": "@fastybird/devices-module",
  "version": "0.2.21",
  "description": "Devices module data model plugin",
  "keywords": [
    "devices",
    "orm",
    "vuex",
    "api"
  ],
  "homepage": "https://www.fastybird.com",
  "bugs": "https://github.com/FastyBird/devices-module/issues",
  "license": "Apache-2.0",
  "author": {
    "name": "FastyBird s.r.o.",
    "email": "info@fastybird.com",
    "url": "https://www.fastybird.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FastyBird/devices-module.git"
  },
  "main": "dist/devices-module.ssr.js",
  "browser": "dist/devices-module.esm.js",
  "module": "dist/devices-module.esm.js",
  "unpkg": "dist/devices-module.min.js",
  "types": "dist/devices-module.d.ts",
  "files": [
    "dist/*",
    "public/locales/*"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "lint:js": "eslint --ext \".js,.ts,.json,\" --ignore-path .gitignore public/",
    "lint:ts": "tslint -c tslint.json 'public/**/*.ts'"
  },
  "dependencies": {
    "@fastybird/modules-metadata": "^0.5",
    "@fastybird/vue-wamp-v1": "^0.1",
    "@fastybird/vuex-orm-wamp": "^0.1",
    "@vuex-orm/core": "^0.36",
    "@vuex-orm/plugin-axios": "^0.9",
    "ajv": "^8.5",
    "jsona": "^1.9",
    "lodash": "^4.17",
    "uuid": "^8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9",
    "@babel/plugin-proposal-class-properties": "^7.10",
    "@babel/preset-env": "^7.9",
    "@babel/preset-typescript": "^7.9",
    "@rollup/plugin-alias": "^2.2",
    "@rollup/plugin-babel": "^5.3",
    "@rollup/plugin-commonjs": "^11.1",
    "@rollup/plugin-eslint": "^8.0",
    "@rollup/plugin-replace": "^2.3",
    "@types/lodash": "^4.14",
    "@types/node": "^15.12",
    "@types/uuid": "^8.3",
    "@typescript-eslint/eslint-plugin": "^4.26",
    "@typescript-eslint/parser": "^4.26",
    "cross-env": "^7.0",
    "eslint": "^7.28",
    "eslint-plugin-json": "^3.0",
    "minimist": "^1.2",
    "rollup": "^2.7",
    "rollup-plugin-dts": "^3.0",
    "rollup-plugin-terser": "^5.3",
    "tslib": "^2.2",
    "tslint": "^6.1",
    "typescript": "^4.3",
    "vuex": "^3.5"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=10"
  }
}
