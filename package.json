    "vue-meta": "^3.0.0-alpha.10",
    "vue-router": "^4.4",
    "vue-tsc": "^2.1"
  },
  "peerDependencies": {
    "pinia": "^2.2",
    "unocss": "^0.64",
    "vue": "^3.5",
    "vue-i18n": "^10.0",
    "vue-meta": "^3.0.0-alpha.10",
    "vue-router": "^4.4"
  },
  "engines": {
    "node": ">=20",
    "fastybird": "1.0.0-dev.24"
  }
