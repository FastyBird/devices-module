    "@nuxt/types": "^2.15.8",
    "@nuxtjs/composition-api": "^0.31.0",
    "@rollup/plugin-alias": "^2.2",
    "@rollup/plugin-babel": "^5.3",
    "@rollup/plugin-commonjs": "^11.1",
    "@rollup/plugin-eslint" : "8.0.2",
    "@rollup/plugin-replace": "^2.3",
    "@types/lodash": "^4.14",
    "@types/node": "^15.12",
    "@types/uuid": "^8.3",
    "@typescript-eslint/eslint-plugin": "^4.26",
    "@typescript-eslint/parser": "^4.26",
    "@zerollup/ts-transform-paths": "^1.7",
    "babel-loader": "^8.2",
    "cross-env": "^7.0",
    "eslint": "^7.28",
    "eslint-plugin-json": "^3.0",
    "minimist": "^1.2",
    "null-loader": "^4.0.1",
    "rollup": "^2.7",
    "rollup-plugin-dts": "^3.0",
    "rollup-plugin-terser": "^5.3",
    "tslib": "^2.2",
    "tslint": "^6.1",
    "typescript": "4.4",
    "vuex": "^3.5"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=10"
  }
}
